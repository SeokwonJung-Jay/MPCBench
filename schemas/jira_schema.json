{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "projects": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["key", "name"],
        "properties": {
          "key": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "fixVersions": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["name", "releaseDate"],
              "properties": {
                "name": {
                  "type": "string"
                },
                "releaseDate": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "issues": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["key", "summary", "status"],
        "properties": {
          "key": {
            "type": "string"
          },
          "summary": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "updated": {
            "type": "string"
          },
          "fixVersions": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "releaseDate": {
                  "type": "string"
                }
              }
            }
          },
          "project": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
